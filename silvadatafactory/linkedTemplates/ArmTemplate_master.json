{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "silvadatafactory"
		},
		"silvadatalake_service_connection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'silvadatalake_service_connection'"
		},
		"silvadataworkspace_silvapooldb_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'silvadataworkspace_silvapooldb_connection'"
		},
		"silvadb_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'silvadb_connection'"
		},
		"silvadatalake_service_connection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://silvadatalake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "silvadatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"silvadatalake_service_connection_accountKey": {
						"value": "[parameters('silvadatalake_service_connection_accountKey')]"
					},
					"silvadataworkspace_silvapooldb_connection_connectionString": {
						"value": "[parameters('silvadataworkspace_silvapooldb_connection_connectionString')]"
					},
					"silvadb_connection_connectionString": {
						"value": "[parameters('silvadb_connection_connectionString')]"
					},
					"silvadatalake_service_connection_properties_typeProperties_url": {
						"value": "[parameters('silvadatalake_service_connection_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "silvadatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'silvadatafactory_ArmTemplate_0')]"
			]
		},
		{
			"name": "silvadatafactory_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'silvadatafactory_ArmTemplate_1')]"
			]
		}
	]
}